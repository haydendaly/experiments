    "import zmq\n",
    "context = zmq.Context()\n",
    "socket = context.socket(zmq.SUB)\n",
    "socket.connect(\"tcp://localhost:5555\")\n",
    "socket.subscribe('11098000')\n",
    "while True:\n",
    "    message = socket.recv_string()\n",
    "    print(\"Data received: {:s}\".format(message))"
